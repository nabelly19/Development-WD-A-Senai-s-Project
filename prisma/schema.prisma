
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Company {
    id Int @id @default(autoincrement())
    name String
    CNPJ String 
    CEP String
    Rua String
    Bairro String
    Cidade String 
    Estado String 
    Pais String 
    Filial Boolean @default(false)
    DataAbertura DateTime
}

model TrainingData {
    id Int @id @default(autoincrement())
    data String
    labels String
}

model MachineLearningModel {
    id Int @id @default(autoincrement())
    modelParameters String
}

model Result {
    id Int @id @default(autoincrement())
    reason String
    approved Boolean
    result String
}